<%- include('partials/header') %>
<div class="editpost">
    <% data.forEach(data=>{%>
        <div class="inpost">
            <div><%=data.title%></div>
            <div><%=data.description%></div>
            <div><%=data.left%></div>
            <div><%=data.right%></div>
            <div><%=data.design%></div>
            <a href="/post/editpost/<%=data._id%>">Edit Post</a>
            <a href="/post/deletepost/<%=data._id%>">Delete Post</a>
        </div>
    <%})%>
</div>
<%- include('partials/footer') %>
